<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <description>MQ Configuration</description>

    <bean id="pureSpot15404_Topic" class="com.blocain.bitms.trade.quotation.consts.TopicConst">
        <constructor-arg name="props" value="quotation/154444444404.properties"/>
    </bean>

    <!-- pureSpot15404_消息接收器 -->
    <bean id="pureSpot15404_KLine1MinMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLine1MinMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLine5MinMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLine5MinMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLine15MinMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLine15MinMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLine30MinMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLine30MinMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLineHourMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLineHourMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLineDayMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLineDayMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLineWeekMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLineWeekMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_KLineMonthMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.KLineMonthMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_DeepPriceMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.DeepPriceMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_RtQuotationMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.RtQuotationMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <bean id="pureSpot15404_RealDealMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.RealDealMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>
    <!-- 全行情消息接收器 -->
    <bean id="pureSpot15404_AllRtQuotationMessageListener"
          class="com.blocain.bitms.trade.quotation.listener.AllRtQuotationMessageListener">
        <property name="quotationWebSocketHandler" ref="pureSpot15404_Handler"/>
    </bean>

    <!-- 监听管理器 -->
    <bean id="pureSpot15404_TopicContainer" class="org.springframework.data.redis.listener.RedisMessageListenerContainer"
          destroy-method="destroy">
        <property name="connectionFactory" ref="jedisConnectionFactory"/>
        <property name="messageListeners">
            <map>
                <!-- pureSpot15404_ -->
                <entry key-ref="pureSpot15404_KLine1MinMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLine1MinChannelTopic" >
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLine5MinMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLine5MinChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLine15MinMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLine15MinChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLine30MinMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLine30MinChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLineHourMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLineHourChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLineDayMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLineDayChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLineWeekMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLineWeekChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_KLineMonthMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.KLineMonthChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_DeepPriceMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.DeepPriceChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_RtQuotationMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.RtQuotationChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <entry key-ref="pureSpot15404_RealDealMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.RealDealChannelTopic">
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
                <!-- 全行情 -->
                <entry key-ref="pureSpot15404_AllRtQuotationMessageListener">
                    <bean class="com.blocain.bitms.trade.quotation.topic.AllRtQuotationChannelTopic" >
                        <constructor-arg name="topicConst" ref="pureSpot15404_Topic" />
                    </bean>
                </entry>
            </map>
        </property>
    </bean>

</beans>